{
    "name": "carsso/ovhcloud",
    "description": "Wrapper for OVHcloud APIs",
    "license": "BSD-3-Clause",
    "config": {
        "sort-packages": true
    },
    "keywords": [
        "api",
        "client",
        "authorization",
        "authorisation",
        "ovh",
        "ovhcloud"
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^6.0||^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "php-parallel-lint/php-parallel-lint": "^1.3.1",
        "phpdocumentor/phpdocumentor": "v3.0.0",
        "squizlabs/php_codesniffer": "^3.6",
        "phpdocumentor/graphviz": "^2.0@dev",
        "symfony/flex": "^1.11"
    },
    "autoload": {
        "psr-4": {"Ovh\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "Ovh\\tests\\": "test/"
        }
    },
    "scripts": {
        "check": "vendor/bin/phpcs -sp",
        "lint": "vendor/bin/phpcbf -sp",
        "parallel-lint": "vendor/bin/parallel-lint src tests",
        "phpunit": "vendor/bin/phpunit"
    }
}
